{
  "name": "splitstack/laravel-metamon",
  "description": "A Laravel package for handling model metadata with role-based access control",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Emilien Kopp",
      "email": "emilien.kopp@gmail.com"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit ./src/Tests/HandlesMetadataTest.php",
    "test:coverage": "vendor/bin/phpunit  Tests/HandlesMetadataTest.php --coverage-html coverage"
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "illuminate/database": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Splitstack\\Metamon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Splitstack\\Metamon\\Tests\\": "src/Tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Splitstack\\Metamon\\MetadataServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
